    <validationRules>
        <fullName>DAP001</fullName>
        <active>true</active>
        <description>If the response is Rejected then make sure they enter a reason in the Comments field.</description>
        <errorConditionFormula>NOT($Permission.Ignore_Validation_Rules) &amp;&amp;(ISPICKVAL( Response__c , 'Reject') &amp;&amp; (ISNULL( Comments__c ) || ISBLANK(Comments__c)))</errorConditionFormula>
        <errorDisplayField>Comments__c</errorDisplayField>
        <errorMessage>DAP001:Please enter a comment for the rejected response</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DAR002</fullName>
        <active>true</active>
        <description>Only assigned user should be able to update the response</description>
        <errorConditionFormula>NOT($Permission.Ignore_Validation_Rules) &amp;&amp;( ISCHANGED( Response__c )&amp;&amp;   $User.Id != OwnerId &amp;&amp; $Profile.Name != 'System Administrator 2FA - custom' &amp;&amp; $Profile.Name != 'System Administrator' )</errorConditionFormula>
        <errorMessage>DAR002: You are not authorized to change the response to this record</errorMessage>
    </validationRules>
</CustomObject>
