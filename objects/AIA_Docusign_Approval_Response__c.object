        <fullName>Approver_Name__c</fullName>
        <externalId>false</externalId>
        <formula>Owner:User.FirstName + ' ' +  Owner:User.LastName</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Approver Name</label>
