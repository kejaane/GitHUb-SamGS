    <validationRules>
        <fullName>GAP001</fullName>
        <active>true</active>
        <description>If the response is Rejected then make sure they enter a reason in the Comments field.</description>
        <errorConditionFormula>Not($Permission.Ignore_Validation_Rules) &amp;&amp; ISPICKVAL(Response__c, 'Reject') &amp;&amp; (ISNULL(Comments__c) || ISBLANK(Comments__c))</errorConditionFormula>
        <errorDisplayField>Comments__c</errorDisplayField>
        <errorMessage>GAP001 : Please enter a comment for the rejected response</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GAP002</fullName>
        <active>true</active>
        <description>Allow user to provide inputs only to respective approval Request</description>
        <errorConditionFormula>Not($Permission.Ignore_Validation_Rules) &amp;&amp; $User.Id  &lt;&gt;  User__r.Id &amp;&amp; NOT(ISNEW()) &amp;&amp; 
 $Profile.Name &lt;&gt; 'System Administrator'&amp;&amp;
Not(Group_Structure_Request_Form__r.ignore_gsa_validations__c )</errorConditionFormula>
        <errorMessage>GAP002 : You don't have access to modify this record</errorMessage>
    </validationRules>
</CustomObject>
