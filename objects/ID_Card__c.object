    <validationRules>
        <fullName>EID01</fullName>
        <active>true</active>
        <description>Approved by Legal to be filled out only when "Will a Non-CareFirst Vendor Logo be Needed?" is Yes</description>
        <errorConditionFormula>AND((ISPICKVAL(Will_a_Non_CareFirst_Vendor_Logo_be_Need__c, "Yes")),ISBLANK(Text( Approved_by_Legal__c)))</errorConditionFormula>
        <errorDisplayField>Approved_by_Legal__c</errorDisplayField>
        <errorMessage>EID01: Please provide a value since this ID Card requires a Non-CareFirst vendor logo</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EID02</fullName>
        <active>false</active>
        <description>At least one record needs to be added to Vendor related list before it can be sent for Approval</description>
        <errorConditionFormula>AND(ISPICKVAL(Will_a_Non_CareFirst_Vendor_Logo_be_Need__c, "Yes"), ISCHANGED(Status__c), Count_of_vendors__c &lt;1)</errorConditionFormula>
        <errorMessage>At least one record needs to be added to Vendor related list before it can be sent for Approval</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EID03</fullName>
        <active>true</active>
        <description>Phone Number field to be populated when "Is there an unique number ?" is Yes</description>
        <errorConditionFormula>AND(ISPICKVAL( Is_there_an_unique_number__c , "Yes"),ISBLANK(Phone_Number__c))</errorConditionFormula>
        <errorDisplayField>Phone_Number__c</errorDisplayField>
        <errorMessage>EID03: Please provide a value since this there is a unique number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EID04</fullName>
        <active>true</active>
        <description>Fill out only when "Are there any special requests ?" is Yes</description>
        <errorConditionFormula>AND( ISPICKVAL( Are_there_any_special_requests__c ,"Yes"), ISBLANK( Special_Requests__c ) )</errorConditionFormula>
        <errorDisplayField>Special_Requests__c</errorDisplayField>
        <errorMessage>EID04: Please provide a value since there are special requests for this ID Card</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EID05</fullName>
        <active>false</active>
        <description>Fill out only when "Is CareFirst Producing ID Card" field is selected as No</description>
        <errorConditionFormula>OR(AND(ISPICKVAL(Is_CareFirst_Producing_ID_Card__c , "Yes"), NOT(ISBLANK( ID_Card_Vendor__c ))),AND(ISPICKVAL(Is_CareFirst_Producing_ID_Card__c , "No"), (ISBLANK( ID_Card_Vendor__c ))))</errorConditionFormula>
        <errorDisplayField>ID_Card_Vendor__c</errorDisplayField>
        <errorMessage>Fill out when "Is CareFirst Producing ID Card" field is selected as No</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EID06</fullName>
        <active>true</active>
        <errorConditionFormula>AND(sent_to_approval__c = TRUE, OR(Count_of_vendors__c = 0, ISBLANK(Count_of_vendors__c ) ), ISPICKVAL( Is_CareFirst_Producing_ID_Card__c ,"No"))</errorConditionFormula>
        <errorMessage>Please add vendors in the related list</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EID07</fullName>
        <active>true</active>
        <errorConditionFormula>AND(sent_to_approval__c = TRUE,OR( ISBLANK(products_selected_count__c),products_selected_count__c=0 ))</errorConditionFormula>
        <errorDisplayField>Member_level_Products__c</errorDisplayField>
        <errorMessage>Please select atleast one product</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EID08</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL( PRIORVALUE( Status__c ) , "In Review") , ( ISPICKVAL( Status__c , "In Progress") ) )</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>In Progress is not allowed when status is in Review</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EID09</fullName>
        <active>false</active>
        <description>Fill out only when "Are there any special requests ?" is Yes</description>
        <errorConditionFormula>AND( ISPICKVAL( Are_there_any_special_requests__c ,"Yes"),( ISBLANK( Special_Requests__c ) ) )</errorConditionFormula>
        <errorDisplayField>Special_Requests__c</errorDisplayField>
        <errorMessage>EID09: Special Requests should be filled when "Are there any special requests ?" is Yes</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EID10</fullName>
        <active>true</active>
        <description>Approved by Legal to be filled out only when "Will a Non-CareFirst Vendor Logo be Needed?" is Yes</description>
        <errorConditionFormula>AND( ISPICKVAL(Will_a_Non_CareFirst_Vendor_Logo_be_Need__c, "No"),!ISBLANK(Text( Approved_by_Legal__c)))</errorConditionFormula>
        <errorDisplayField>Approved_by_Legal__c</errorDisplayField>
        <errorMessage>EID10: To be filled out  when "Will a Non-CareFirst Vendor Logo be Needed?" is Yes</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EID11</fullName>
        <active>true</active>
        <description>approver cannot select other than approved or rejected in status field</description>
        <errorConditionFormula>Not($Permission.Ignore_Validation_Rules) &amp;&amp; NOT((ISPICKVAL( Status__c  , 'Approved') || ISPICKVAL( Status__c , 'Rejected'))) &amp;&amp; OwnerId  != $User.Id</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>EID11: Approver cannot select other than approved or rejected in status field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EID12</fullName>
        <active>true</active>
        <description>User cannot set status to Approved/Rejected</description>
        <errorConditionFormula>/*AND(OwnerId = $User.Id,  (ISPICKVAL( Status__c , "Approved")|| ISPICKVAL( Status__c , "Rejected")|| ISPICKVAL( Status__c , "In Review")))*/

AND(OwnerId = $User.Id,  (ISPICKVAL( Status__c , "Approved")|| ISPICKVAL( Status__c , "Rejected")|| ISPICKVAL( Status__c , "In Review")), ISCHANGED(Status__c) )</errorConditionFormula>
        <errorMessage>EID: User can only set status to "In Progress"</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EID13</fullName>
        <active>true</active>
        <description>Approver cannot set the status to In Progress</description>
        <errorConditionFormula>Not($Permission.Ignore_Validation_Rules) &amp;&amp; AND(OwnerId != $User.Id, NOT(ISPICKVAL( Status__c , "Approved")||ISPICKVAL( Status__c , "Rejected") ))</errorConditionFormula>
        <errorMessage>EID13: Approver can set status only to either Approved or Rejected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EID14</fullName>
        <active>true</active>
        <description>When status is Approved or Rejected or In-Review, it cannot be submitted again</description>
        <errorConditionFormula>/*AND(OR(TEXT(Status__c)='In Review',TEXT(Status__c)='Approved',TEXT(Status__c)='Rejected'), sent_to_approval__c =true )*/
AND(OR(ISPICKVAL(PRIORVALUE(Status__c),'Approved'),ISPICKVAL(PRIORVALUE(Status__c),'Rejected')), sent_to_approval__c =true)</errorConditionFormula>
        <errorMessage>EID14: When status is Approved or Rejected or In-Review, it cannot be submitted again</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EID15</fullName>
        <active>true</active>
        <description>When Status is Rejected, Comments needs to be added</description>
        <errorConditionFormula>AND(Text(Status__c) ='Rejected', ISBLANK(Approval_Status_Comments__c))</errorConditionFormula>
        <errorDisplayField>Approval_Status_Comments__c</errorDisplayField>
        <errorMessage>EID15:When Status is Rejected, Comments needs to be added</errorMessage>
    </validationRules>
