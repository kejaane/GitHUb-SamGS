    <fields>
        <fullName>Number__c</fullName>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <label>Number</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Record_Description__c</fullName>
        <description>Enter a description that describes the records purpose.</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <inlineHelpText>Enter a detailed description that describes the records purpose and allows the user to understand how to use and which support field(s) are needed.</inlineHelpText>
        <label>Record Description</label>
        <length>32768</length>
        <type>LongTextArea</type>
        <visibleLines>4</visibleLines>
