        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Application Level</fullName>
                    <default>true</default>
                    <label>Application Level</label>
                </value>
                <value>
                    <fullName>Deprovision User</fullName>
                    <default>false</default>
                    <label>Deprovision User</label>
                </value>
                <value>
                    <fullName>Profile Audit</fullName>
                    <default>false</default>
                    <label>Profile Audit</label>
                </value>
                <value>
