    <validationRules>
        <fullName>EWPTT001</fullName>
        <active>true</active>
        <description>When Task Owner Type is QUEUE, Queue Name cannot be blank</description>
        <errorConditionFormula>Not($Permission.Ignore_Validation_Rules) &amp;&amp; (AND(TEXT(Task_Owner_Type__c)='QUEUE', ISBLANK(TEXT(Queue_Name__c ))))</errorConditionFormula>
