    <validationRules>
        <fullName>WPT001</fullName>
        <active>true</active>
        <description>User cannot change the status once it is "Completed"</description>
        <errorConditionFormula>Not($Permission.Ignore_Validation_Rules) &amp;&amp;(ISPICKVAL(PRIORVALUE( Status__c ),"Completed")&amp;&amp; ISCHANGED( Status__c ))</errorConditionFormula>
        <errorMessage>WPT001: User cannot change the status once it is "Completed"</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>WPT002</fullName>
        <active>true</active>
        <description>User cannot change the status to "Blocked" from detail page</description>
        <errorConditionFormula>Not(IsUpdateFromVF__c) &amp;&amp; ISPICKVAL(Status__c,"Blocked")</errorConditionFormula>
        <errorMessage>WPT002: User cannot change the status to "Blocked" from Detail page</errorMessage>
    </validationRules>
