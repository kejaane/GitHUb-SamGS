function onChangeMandateYesNo(element,isPanelClick)      
{ 
    if({!QAComplete} == true){ 
    if(isPanelClick == 'true')
    {
        if(element.value == 'Yes')
        {
            $(element).closest('tr').nextAll().find('.Limitaion').each(function () {        
            var parentText = $(element).closest('tr').text();       
            var LimitaionText = $(this).closest('tr').text();       
            var parentName = parentText.substr(0, parentText.indexOf(" "));     
            var LimitaionName = LimitaionText.substr(0, LimitaionText.indexOf(" "));        
                if(parentName==LimitaionName){      
                     $(this).closest('tr').find('input[type="checkbox"]').each(function () {        
                        if($(this).closest('tr').css("display")=="table-row"){      
                             var tdtxt =  $(this).parent('td').text();      
                             tdtxt = tdtxt.replace("--None--","");              
                             tdtxt = tdtxt.replace("--none--","");          
                             var res = tdtxt.split("--");       
                                    
                             if(res.length > 0 && res[0].trim().length > 0)     
                             {      
                                $(this).css("pointer-events", "None");                                         
                                 $(this).removeAttr("checked");
                             }      
                         }      
                     });        
                 }      
            });
        }
    }
    else if(element.value == 'Yes')      
    {       
        var ele = $(element).closest('tr').find('.Jurisdiction').find('option:selected')[0];        
              var currentVal = ele.value        
                     $(ele).closest('tr').find('input[type="checkbox"]').each(function () {     
                        if($(this).closest('tr').css("display")=="table-row"){      
                         var tdtxt =  $(this).parent('td').text();      
                         tdtxt = tdtxt.replace("--None--","");              
                         tdtxt = tdtxt.replace("--none--","");          
                         var res = tdtxt.split("--");       
                                
                         if(res.length > 0 && res[0].trim().length > 0)     
                         {      
                                    
                             if(res[1].indexOf(currentVal)>=0 && currentVal.length>0)       
                             {      
                                if('{!pp.Funding_Arrangement__c}' != 'Risk'){    
                                	$(this).css("pointer-events", "auto");                                         
                                    $(this).removeAttr("disabled");     
                                    $(this).prop("checked", true);      
